{
  "name": "survey-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node src/main.js",
    "start:watch": "nodemon --ignore tests/* src/main.js",
    "test": "mocha test/**/*.test.js",
    "test:watch": "nodemon --ext js --exec 'clear && npm run test'",
    "test:cov": "nyc mocha test/**/*.test.js",
    "lint": "eslint ./**/*.js",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mscamargo/survey-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mscamargo/survey-api/issues"
  },
  "homepage": "https://github.com/mscamargo/survey-api#readme",
  "imports": {
    "#src/*": "./src/*"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "mocha": true
    }
  },
  "devDependencies": {
    "eslint-config-standard": "^17.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "supertest": "^6.2.3"
  }
}
